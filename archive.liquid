---
pagination:
  data: collections.posts
  size: 50
  alias: posts
layout: layout.liquid
---

<div id="archive">
  <ol>
    {% for post in posts %}

    <li>
      <a href="{{ post.url }}">{{ post.data.title }}</a>
      <span>{{ post.date | date: "%B %d, %Y" }}</span>
    </li>

    {% endfor %}
  </ol>
</div>
